{
    "name": "heyitspavel/fitbitphp",
    "type": "library",
    "description": "Zend Framework 2 module for easy integration of Fitbit",
    "keywords": ["Zend Framework 2","Fitbit","bundle"],
    "license": "MIT",
    "minimum-stability" : "stable",
    "support": {
        "issues": "https://github.com/WebpageFX/fitbitphp/issues"
    },
    "repositories":[
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "heyitspavel/fitbitphp",
                "source": {"url": "https://github.com/webpagefx/fitbitphp.git","type": "git","reference": "master"}
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*@stable"
    },
    "require-dev": {
    	"phpunit/phpunit": "3.*@stable",
    	"satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "fitbitphp": "src"
        },
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
